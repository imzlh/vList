{
    "prefix":"/@list/",
    "thumb_prefix":false,
    "debug":true,
    "animate":true,
    "autoBack":true,
    "search":false,
    "autoTheme":true,
    "globalFont":16,
    "map":{
        "text": {
            "subtitle": [
                "vtt",
                "ass",
                "srt"
            ],
            "lyrics": ["lrc"],
            "markdown": [
                "md"
            ],
            "code":[
                "php",
                "js","css","html","xhtml",
                "scss","ts",
                "json","ini","xml","yaml",
                "c","cpp","h",
                "java",
                "go",
                "make",
                "perl","pl",
                "sh","bash","cmd","bat","vbs",
                "dockerfile","docker"
            ],
            "plain": [
                "log",
                "txt"
            ],
            "cd":["cue"]
        },
        "video": {
            "video": [
                "webm",
                "mp4",
                "mkv",
                "avi",
                "ts",
                "flv",
                "m3u8"
            ]
        },
        "audio": {
            "audio": [
                "mp3",
                "aac", "m4a",
                "opus",
                "wav",
                "flac"
            ]
        },
        "image":{
            "image":[
                "avif",
                "webp",
                "jpg","jpeg","jxl",
                "png",
                "ico",
                "bmp"
            ]
        }
    },
    "opener":{
        "audio":"opener/vplayer.js",
        "video":"opener/artplayer.js",
        "cd":"opener/vplayer.js",
        "image":"opener/viewer.js",
        "text":"opener/highlight.js"
    },
    "extension":[
        "module/fcheck.js",
        "module/setting.js"
    ],
    "hide" : [
        "text/subtitle",
        "text/lyrics"
    ],
    "viewer":{
        "delay":10,
        "zoomRatio":0.25,
        "zoomRange":[0.01,5]
    },
    "video":{
        "playback":true,
        "notice":3000,
        "rate":[0.8,1,1.2,1.5,2],
        "currect":"",
        "seekStep":10,
        "status":{},
        "enableAss":true
    }
}