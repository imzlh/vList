{
    "prefix":"/@list/",
    "thumb_prefix":false,
    "debug":false,
    "version":3.2,
    "alert":"欢迎来到izCloud!<br><a href='https://io.imzlh.top:8443/'>备用站点</a>",
    "alert_delay":5000,
    "animate":true,
    "autoBack":true,
    "search":false,
    "autoTheme":true,
    "autohide":true,
    "globalFont":16,
    "packerPath":"vendor/packer/",
    "map":{
        "text": {
            "subtitle": [
                "vtt",
                "ass",
                "srt"
            ],
            "lyrics": ["lrc"],
            "markdown": [
                "md"
            ],
            "code":[
                "php",
                "js","css",
                "scss","ts",
                "json","ini","xml","yaml",
                "c","cpp","h",
                "java",
                "go",
                "make",
                "perl","pl",
                "sh","bash","cmd","bat","vbs",
                "dockerfile","docker"
            ],
            "plain": [
                "log",
                "txt",
                "md5",
                "sfv",
                "nfo","info"
            ],
            "cd":["cue"],
            "h5page":["html","xhtml"],
            "h5link":["url"]
        },
        "video": {
            "video": [
                "webm",
                "mp4",
                "mkv",
                "avi",
                "ogv"
            ],
            "depvideo":[
                "ts",
                "flv",
                "m3u8"
            ]
        },
        "audio": {
            "audio": [
                "mp3",
                "aac", "m4a",
                "opus",
                "wav",
                "flac",
                "ogg",
                "caf"
            ],"unsupport":[
                "tak",
                "wv",
                "ape"
            ]
        },
        "image":{
            "image":[
                "avif",
                "webp",
                "jpg","jpeg","jxl",
                "png",
                "ico",
                "bmp"
            ]
        }
    },
    "opener":{
        "audio":"opener/aplayer.js",
        "video":"opener/hplayer.js",
        "depvideo":"opener/artplayer.js",
        "cd":"opener/vplayer.js",
        "image":"opener/viewer.js",
        "text":"opener/highlight.js"
    },
    "extension":[
        "module/fcheck.js",
        "module/setting.js",
        "opener/h5api.js",
        "module/adalert.js"
    ],
    "hide" : [
        "text/subtitle",
        "text/lyrics"
    ],
    "viewer":{
        "delay":10,
        "zoomRatio":0.25,
        "zoomRange":[0.01,5]
    },
    "video":{
        "playback":true,
        "notice":3000,
        "rate":[0.8,1,1.2,1.5,2],
        "seekStep":10,
        "seeker":true,
        "status":{},
        "enableAss":true
    },
    "admin":{
        "prefix":"/@dav/",
        "enable":false
    }
}